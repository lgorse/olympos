<div class = "explain_wrapper">
  <div class = "app_explain">
    <h1><%= "Welcome to #{APP_NAME}" %></h1>
    <h2>Find awesome squash partners in your area</h2>
  </div>
</div>
<div class = "signup_wrapper">
  <div id = "session_signup" class = "round">
    <%= render 'fb_scripts/fb_login' %>
    <%= render 'session_form' %>
    <h3>Don't have an account?</h3>
    <div class = "new_user_signup">
      <%= render 'fb_scripts/fb_signup' %>
      <%= link_to 'Sign up with Email', new_user_path %>
    </div>
  </div>
</div>
