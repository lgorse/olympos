$(function(){
	switch("<%= params[:edit_type] %>"){
		case "<%= INTRO %>":
		render_info2();
		break;
		case "<%= LOCATION %>":
		render_user_location();
		break;
		case "<%= FIRST_RATING %>":
		render_user_first_rating();
		default :

	}

})

function render_info2(){
	$(".user_info2").
	html("<%= escape_javascript(render 'users/user_profile_partials/user_info2') %>");

}

function render_user_location(){
	var wrapper = $(".user_info .user_location");
	wrapper.
	html("<%= escape_javascript(render 'users/user_profile_partials/user_location') %>");
}

function render_user_first_rating(){
	var wrapper = $(".user_info .skill_rate_prompt");
	wrapper.remove();
}

