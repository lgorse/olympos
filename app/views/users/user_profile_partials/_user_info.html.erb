<div class = "user_ratings">
  <% if current_user?(@user) && @user.first_rating.blank? %>
    <div class = "skill_rate_prompt">
  <p>Set your skill level below:</p>
  
</div>

      <%= render 'users/user_edit_partials/first_rating', :user => @user %>
    
  <% else %>
    <%= render 'layouts/ratings/show_rating', :star_size => "stars", :rating => @user.first_rating,
  :rating_type => "Skill" %>
  <% end %>
  <%= render 'layouts/ratings/show_rating', :star_size => "stars", :rating => @user.fairness, :rating_type => "Fairness" %>
</div>
<div class = 'user_location'>
 <%= render 'users/user_profile_partials/user_location' %>
</div>
<div class = "dialog_about_skill">
  <div class = "about_skill_wrapper"></div></div>
