<div class = "photo_wrapper">
<%= render 'users/user_photo_partials/user_profile_picture' %>
</div>
<%= form_for @current_user do |user| %>
<%= hidden_field_tag :redirect_url, home_users_path%>
<ul>
	 <% if @current_user.first_rating.blank? %>
	<li>
		<%= render 'layouts/ratings/first_rating', :user => user %>

	</li>
	 <% end %>
	<li>
		<%= user.label :zip, "Your location" %>
		<%= user.text_field :zip, :placeholder => "zip code" %>
	</li>
	<li>
		<%= user.submit 'Done' %>
	</li>
	
</ul>
<% end %>